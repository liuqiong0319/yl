using System.Web.UI.WebControls;

namespace WebHyDyf.ASPX.HouTai
{
    public partial class HouTaiADM : System.Web.UI.Page
    {
        protected void Page_Load(object sender, EventArgs e)
        {
            if (!IsPostBack)
            {

            }
        }

        protected void btnSave_Click(object sender, EventArgs e)
        {
            string sql = @"insert into dyf_ypdm(ypmc,cdmc,gg,jx,bz,zbz,dw,pzwh,lsj) values
('" + txtYPMC.Text.ToString().Trim() + "','" + txtCDMC.Text.ToString().Trim() + "','" + txtGG.Text.ToString().Trim() + "','" + txtJX.Text.ToString().Trim() + "'," + int.Parse(txtBZ.Text.ToString().Trim()) + "," + int.Parse(txtZBZ.Text.ToString().Trim()) + ",'" + txtDW.Text.ToString().Trim() + "','" + txtPZWH.Text.ToString().Trim() + "'," + float.Parse(txtLSJ.Text.ToString().Trim()) + ")";

            int num = HyeyClass.DBUtility.DbHelperSQL.ExecuteSql(sql);
            if (num > 0)
            {
                MessageBox("上传成功！");
            }

        }
        protected void MessageBox(string str)
        {
            Page.ClientScript.RegisterStartupScript(Page.GetType(), "message", "<script language='javascript' defer>alert('" + str + "');</script>");
        }
    }
}