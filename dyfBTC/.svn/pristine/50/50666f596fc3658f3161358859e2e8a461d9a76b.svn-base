.ShenHe {
}

.ShenHe-Table {
    width: 990px;
    border-collapse: collapse;
    border-spacing: 0px;
}

    .ShenHe-Table th {
        border: 1px solid #c4c4c4;
        background-color: #ededed;
        font-size: 13px;
        line-height: 30px;
    }

    .ShenHe-Table td {
        text-indent: 0.5em;
        line-height: 20px;
        font-size: 12px;
        border: 1px solid #c4c4c4;
        padding-top: 3px;
    }

    .ShenHe-Table tr {
        background-color: #fff0eb;
        border-top: 1px solid #c4c4c4;
    }

        .ShenHe-Table tr:nth-child(even) {
            background: #ffffff;
            border: 1px solid #c4c4c4;
        }

        .ShenHe-Table tr:hover {
            background-color: #ffd8c0;
            border: 1px solid #c4c4c4;
        }


    .ShenHe-Table td span {
        color: #808080;
        margin-left: 5px;
    }

.ShenHe-Table_td02 {
    text-align: right;
    color: #f00;
    padding-right: 8px;
}
